        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/website_formio/static/src/js/website_formio_editor.js"/>
        </xpath>
    </template>

    <template id="assets_frontend_website_formio" inherit_id="website.assets_frontend">
